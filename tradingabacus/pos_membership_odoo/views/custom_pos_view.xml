<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
	
		
		<record id = "pos_report_inherit_search_view" model = "ir.ui.view">
		 	<field name = "name">pos.report.search.inherit</field>
		 	<field name = "model">report.pos.order</field>
		 	<field name = "inherit_id" ref = "point_of_sale.view_report_pos_order_search"/>
		 	<field name = "arch" type ="xml">
		 		<xpath expr="//filter[@name='User']" position="after">
		 			<filter string = "Membership Card" name="Membership Card"
		 				context= "{'group_by': 'pos_order_membership_code'}" />
		 		</xpath>
		 	</field>
		</record>

		
	</data>
</odoo>
